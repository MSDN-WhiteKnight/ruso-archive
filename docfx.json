{  
  "build": {
    "content": [      
      {
        "files": [
          "deleted-messages/**.md",		  
          "deleted-messages/**/toc.yml",
		  
		  "tools/html/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
		  "tools/html/ru.stackoverflow.com/posts/*.html",
          "images/**"
        ]
      }
    ],    	
    "dest": "html",
	"globalMetadata": {
      "_appTitle": "RuSO Archive",
      "_appFooter": "<span>RuSO Archive (<a href=\"https://github.com/MSDN-WhiteKnight/ruso-archive\">GitHub repository</a>). Some content is retrieved automatically from StackExchenge API. Licensed under CC-BY-SA 4.0.<br>Generated by <strong>DocFX</strong></span>",      
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
